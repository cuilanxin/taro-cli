{
  "name": "market-h5",
  "version": "1.0.0",
  "private": true,
  "description": "taro",
  "templateInfo": {
    "name": "redux",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "start": "npm run build:weapp -- --watch",
    "build": "npm run build:weapp",
    "file": "taro create --name",
    "version": "npm run taro --version",
    "mock": "mocker ./mocker/api.js --host localhost --port 9527",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "style:fix": "stylelint --fix 'src/*.{css,scss}' ",
    "prepare": "husky install",
    "lint": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.3.5",
    "@tarojs/react": "3.3.5",
    "@tarojs/redux": "^2.2.10",
    "@tarojs/redux-h5": "^2.2.10",
    "@tarojs/runtime": "3.3.5",
    "@tarojs/taro": "3.3.5",
    "dva-core": "^2.0.4",
    "dva-loading": "^3.0.22",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "taro-ui": "^3.0.0-alpha.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@tarojs/cli": "^3.3.5",
    "@tarojs/mini-runner": "3.3.5",
    "@tarojs/webpack-runner": "3.3.5",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.13.6",
    "babel-eslint": "10.1.0",
    "babel-preset-taro": "3.3.5",
    "eslint": "7.30.0",
    "eslint-config-alloy": "4.1.0",
    "eslint-config-taro": "3.3.5",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.1",
    "mocker-api": "2.9.0",
    "prettier": "2.3.2",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-taro-rn": "2.2.17",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "stylelint-taro-rn": "2.2.17"
  }
}
